{
  "server": {
    "listen": ":8443",
    "tls": {
      "cert": "cert.pem",
      "key": "key.pem",
      "sni_fake": true,
      "fake_snis": ["cloudflare.com"]
    }
  },
  "auth": {
    "method": "psk",
    "key": "test-key-123456789012345678901234567890"
  },
  "exit_ips": ["127.0.0.1", "127.0.0.2"],
  "strategy": {
    "type": "round_robin"
  },
  "snat": {
    "enabled": false,
    "gateway": "192.168.1.1",
    "interface": "eth0"
  },
  "logging": {
    "level": "info",
    "file": "multiexit-proxy.log"
  },
  "web": {
    "enabled": true,
    "listen": ":8080",
    "username": "admin",
    "password": "admin123"
  }
}
